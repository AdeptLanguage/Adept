
import 'sys/cstdio.adept'

func main(in argc int, in argv **ubyte) int {
    if argc == 1, puts('No args specified'); return 1
    if argc == 2, puts('Only one arg specified'); return 1
    puts('More than one arg specified')

    i int = 0; while i != 10, i += 1; printf('%d\n', i)

    if i == 0, puts('Failed to get off the ground')
    else if i > 0 && i < 10, puts('Failed to completely launch')
    else puts('Launched successfully')
    return 0
}
