
import 'sys/cstdio.adept'
import 'sys/cstring.adept'

struct Human (name 256 ubyte)

func main(in argc int, in argv **ubyte) int {
    human Human
    memcpy(&human.name, 'Billy', 6)
    printf('human.name : %s %d\n', &human.name)

    buffer 512 ubyte
    message *ubyte = 'Hello World! This is a test message in a static buffer'
    memcpy(&buffer, message, strlen(message) + 1)
    printf('%s\n', &buffer)
    return 0
}
