
import String
import cstring

func adept\mustBeUnsigned64(string String, is_hex bool) bool {
    // NOTE: Doesn't care whether 'string' is valid integer representation
    
    if is_hex {
        // Base 16
        return string.length != 16 ? string.length > 16 : strncmp(string.array, '7FFFFFFFFFFFFFFF', 16) > 0
    } else {
        // Base 10
        return string.length != 19 ? string.length > 19 : strncmp(string.array, '9223372036854775807', 19) > 0
    }
}
