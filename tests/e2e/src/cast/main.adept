    uint2ulong   ulong = cast ulong 21ui
    long2ulong   ulong = cast ulong 21sl
    //ulong2ulong  ulong = cast ulong 21ul
    float2ulong  ulong = cast ulong 21.0f
    double2ulong ulong = cast ulong 21.0d

    // ========== to 'float' ==========
