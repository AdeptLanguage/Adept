    byte2float   float = cast float 21sb
    ubyte2float  float = cast float 21ub
    short2float  float = cast float 21ss
    ushort2float float = cast float 21us
    int2float    float = cast float 21si
    uint2float   float = cast float 21ui
    long2float   float = cast float 21sl
    ulong2float  float = cast float 21ul
    //float2float  float = cast float 21.0f
    double2float float = cast float 21.0d

    // ========== to 'double' ==========
    bool2double   double = cast double true
    byte2double   double = cast double 21sb
    ubyte2double  double = cast double 21ub
    short2double  double = cast double 21ss
    ushort2double double = cast double 21us
    int2double    double = cast double 21si
    uint2double   double = cast double 21ui
    long2double   double = cast double 21sl
    ulong2double  double = cast double 21ul
    float2double  double = cast double 21.0f
    //double2double double = cast double 21.0d
    return 0
}
