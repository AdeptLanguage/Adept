
import basics

func main {
    // More traditional version
    for(i int = 0; i < 10; i++){
        printf("%d ", i)
    }
    newline()

    // More traditional version, Single Line
    for(i int = 0; i < 10; i++), printf("%d ", i)
    newline()

    // Without parentheses
    for i int = 0; i < 10; i++ {
        printf("%d ", i)
    }

    // Without parentheses, Single Line
    for i int = 0; i < 10; i++, printf("%d ", i)
    newline()
}
