
import 'sys/cstdio.adept'

func main {
    printf('%f %f %f\n', sum(), sum(10.0), sum(10.0, 11.0))
}

func sum(a double = 0.0, b double = 0.0, c double = 0.0) double {
    return a + b + c
}

